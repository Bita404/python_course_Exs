def num_multiply(num1 , num2) :
    if num1 < num2 :
        return num1 * num_multiply(num1 +1 , num2)
    elif num1 > num2 :
        return num2 * num_multiply(num1 , num2 +1 )    
    else :
        return num1 
    
n1 = int(input("Enter your first Number :"))
n2 = int(input("Enter your 2nd Number : "))

num_multiply(n1 ,n2 )    
